FirstName(\"\");\n    setLastName(\"\");\n    setEmail(\"\");\n  }\n\n  const logo512 = process.env.PUBLIC_URL + \"logo512.png\";\n  const nodeEnvironemt = process.env.NODE_ENV;\n  const child = process.env.REACT_APP_CHILD;\n\n  console.log(logo512, nodeEnvironemt, child);\n\n  return (\n    <div>\n      <img src={logo512} alt=\"logo512\" />\n      {Object.keys(errors).length > 0 && (\n        <Errors errors={Object.values(errors)} />\n      )}\n      <form onSubmit={handleSubmit} className={styles.form}>\n        <div>\n          <label htmlFor=\"firstName\">First name</label>\n          <input\n            type=\"text\"\n            id=\"firstName\"\n            value={firstName}\n            onChange={(event) => setFirstName(event.target.value)}\n          />\n        </div>\n\n        <div>\n          <label htmlFor=\"lastName\">Last name</label>\n          <input\n            type=\"text\"\n            id=\"lastName\"\n            value={lastName}\n            onChange={(event) => setLastName(event.target.value)}\n          />\n        </div>\n\n        <div>\n          <label htmlFor=\"email\">Email</label>\n          <input\n            type=\"text\"\n            id=\"email\"\n            value={email}\n            onChange={(event) => setEmail(event.target.value)}\n          />\n        </div>\n\n        <div>\n          <button type=\"submit\">Submit</button>\n          <Button type=\"submit\">Custom submit</Button>\n          <BButton variant=\"primary\">Primary</BButton>\n        </div>\n      </form>\n    </div>\n  );\n}\n","C:\\Users\\gonzalo\\projects\\react\\my-other-app\\src\\stories\\Header.stories.js",["218"],"C:\\Users\\gonzalo\\projects\\react\\my-other-app\\src\\stories\\Page.stories.js",["219"],"C:\\Users\\gonzalo\\projects\\react\\my-other-app\\src\\stories\\Button.stories.js",["220"],"C:\\Users\\gonzalo\\projects\\react\\my-other-app\\src\\stories\\Page.js",[],"C:\\Users\\gonzalo\\projects\\react\\my-other-app\\src\\stories\\Header.js",[],"C: